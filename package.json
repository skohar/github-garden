    "url": "https://github.com/skohar/github-garden/issues"
  },
  "homepage": "https://github.com/skohar/github-garden#readme",
  "devDependencies": {
    "electron-prebuilt": "^0.36.10"
  },
  "dependencies": {
    "babel-core": "^5.8.29",
    "electron-packager": "^5.2.1",
    "jquery": "^2.1.4",
    "menubar": "^2.3.0",
    "moment": "^2.10.6",
    "react": "^0.14.0",
    "react-dom": "^0.14.0",
    "react-timer-mixin": "^0.13.3"
  }
