  "description": "A viewer for current own grass color",
  "engines": {
    "node": "5.0.0"
  },
  "main": "main.js",
  "scripts": {
    "start": "$(npm bin)/electron .",
    "build": "$(npm bin)/electron-packager . github-garden --out dist --platform=darwin --arch=x64 --version=0.36.10 --overwrite",
    "deploy": "bash deploy.sh"
  },
  "repository": {
    "type": "git",
    "url": "git+https@github.com:skohar/github-garden.git"
  },
  "author": "skohar",
